         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Text>
      <TextField fx:id="textFieldNombre2" maxWidth="-Infinity" promptText="nombre" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Email" />
      <TextField fx:id="textFieldEmail3" maxHeight="-Infinity" maxWidth="-Infinity" promptText="email" />
      <Button fx:id="buttonVolverAtras" mnemonicParsing="false" onAction="#onClickVolver" text="volver atras" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
